/* pages/common/common.wxss */
Page{
  height:100%;
}
.container{
  width:100%;
  height:100%
}

.item {
  width: 100%;
  height: 75rpx;
  position: relative;
}

.btn {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  right: 0;
  opacity: 0;
}

.left {
  float: left;
  width: 75rpx;
  height: 100%;
}

.left_pic {
  width: 40rpx;
  height: 40rpx;
  margin-top: 17.5rpx;
}

.middle {
  float: left;
  height: 100%;
  box-sizing: border-box;
  line-height: 75rpx;
  font-size: 29rpx;
  letter-spacing: 3rpx;
  font-weight: 400;
}

.right {
  float: right;
  width: 75rpx;
  height: 100%;
}

.pic {
  width: 30rpx;
  height: 30rpx;
  margin-top: 22.5rpx;
}

.box {
  width: 90%;
  /* height:400rpx; */
  margin: 60rpx auto;
  margin-bottom: 0;
  background: rgb(250, 250, 249);
  border-radius: 15rpx;
  box-shadow: 0 0 1 1 white;
  overflow: hidden;
  box-sizing: border-box;
  padding-left: 17rpx;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
}

.line {
  width: 100%;
  height: 2rpx;
  background: white;
}